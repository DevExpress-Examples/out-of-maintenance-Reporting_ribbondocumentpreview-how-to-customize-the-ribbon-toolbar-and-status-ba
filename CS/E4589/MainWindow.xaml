<Window x:Class="E4589.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:dxp="http://schemas.devexpress.com/winfx/2008/xaml/printing" 
        xmlns:local="clr-namespace:E4589"
        Title="MainWindow" Height="350" Width="525">
    <Window.DataContext>
        <local:ViewModel />
    </Window.DataContext>
    <Window.Resources>
        <ResourceDictionary Source="Resources.xaml"/>
    </Window.Resources>
    <dxp:RibbonDocumentPreview  Name="documentPreview" Model="{Binding PreviewModel}" dxp:RibbonCustomization.Template="{StaticResource RibbonCustomizationTemplate}" />
</Window>
 